{
	"name": "isokandonee/fixas-app",
	"description": "Fixas-Bank web application",
	"type": "project",
	"require": {
		"php": "^7.4",
		"vlucas/phpdotenv": "^5.5",
		"monolog/monolog": "^2.9"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.5",
		"phpstan/phpstan": "^1.10"
	},
	"autoload": {
		"psr-4": {
			"App\\": "src/"
		},
		"files": [
			"src/helpers.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"scripts": {
		"test": "phpunit"
	}
}
